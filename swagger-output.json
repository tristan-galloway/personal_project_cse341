{
  "swagger": "2.0",
  "info": {
    "title": "Contacts API",
    "description": "API for managing contacts with CRUD operations.",
    "version": "1.0.0"
  },
  "basePath": "/",
  "schemes": [
    "http"
  ],
  "paths": {
    "/contacts/": {
      "get": {
        "description": "Get all contacts",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "List of contacts",
            "schema": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "_id": {
                    "type": "string",
                    "example": "6816bf25769e8f5a9c4a6a0b"
                  },
                  "firstName": {
                    "type": "string",
                    "example": "Alice"
                  },
                  "lastName": {
                    "type": "string",
                    "example": "Johnson"
                  },
                  "email": {
                    "type": "string",
                    "example": "alicejohnson@test.com"
                  },
                  "favoriteColor": {
                    "type": "string",
                    "example": "red"
                  },
                  "birthdate": {
                    "type": "string",
                    "example": "02/02/22"
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "post": {
        "description": "Create a new contact",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "Contact data",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "type": "string",
                  "example": "Alice"
                },
                "lastName": {
                  "type": "string",
                  "example": "Johnson"
                },
                "email": {
                  "type": "string",
                  "example": "alicejohnson@test.com"
                },
                "favoriteColor": {
                  "type": "string",
                  "example": "red"
                },
                "birthdate": {
                  "type": "string",
                  "example": "02/02/22"
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Contact created",
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "type": "string",
                  "example": "Alice"
                },
                "lastName": {
                  "type": "string",
                  "example": "Johnson"
                },
                "email": {
                  "type": "string",
                  "example": "alicejohnson@test.com"
                },
                "favoriteColor": {
                  "type": "string",
                  "example": "red"
                },
                "birthdate": {
                  "type": "string",
                  "example": "02/02/22"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "Missing a required field."
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/contacts/{id}": {
      "get": {
        "description": "Get a single contact by ID",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Contact ID",
            "example": "609e12672f8fb814b56fa181"
          }
        ],
        "responses": {
          "200": {
            "description": "Contact found",
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "type": "string",
                  "example": "Jane"
                },
                "lastName": {
                  "type": "string",
                  "example": "Smith"
                },
                "email": {
                  "type": "string",
                  "example": "janesmith@test.com"
                },
                "favoriteColor": {
                  "type": "string",
                  "example": "green"
                },
                "birthdate": {
                  "type": "string",
                  "example": "01/01/21"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "404": {
            "description": "Contact not found"
          }
        }
      },
      "put": {
        "description": "Update a contact by ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Contact ID",
            "example": "609e12672f8fb814b56fa181"
          },
          {
            "name": "body",
            "in": "body",
            "description": "Updated contact data",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "type": "string",
                  "example": "John"
                },
                "lastName": {
                  "type": "string",
                  "example": "Doe"
                },
                "email": {
                  "type": "string",
                  "example": "johndoe@test.com"
                },
                "favoriteColor": {
                  "type": "string",
                  "example": "blue"
                },
                "birthdate": {
                  "type": "string",
                  "example": "12/12/20"
                }
              }
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Contact updated"
          },
          "404": {
            "description": "Contact not found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "delete": {
        "description": "Delete a contact by ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Contact ID",
            "example": "609e12672f8fb814b56fa181"
          }
        ],
        "responses": {
          "200": {
            "description": "Contact deleted"
          },
          "404": {
            "description": "Contact not found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/api-docs/": {
      "get": {
        "description": "",
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    }
  }
}